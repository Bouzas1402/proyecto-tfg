[
  {
    "group": "ADMIN_ROLE",
    "permissions": [
      {
        "resource": "*",
        "methods": "*",
        "action": "allow"
      }
    ]
  },
  {
    "group": "USER_ROLE",
    "permissions": [
      {
        "resource": "/proyecto/anuncios",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/comprobarToken",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/anunciosguardados",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/guardarAnuncios/*",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/borraranuncioguardado/*",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/user",
        "methods": ["DELETE", "PUT"],
        "action": "allow"
      }
    ]
  },
  {
    "group": "VENTAS_ROLE",
    "permissions": [
      {
        "resource": "/proyecto/anuncios",
        "methods": ["GET", "POST", "DELETE"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/anunciosubidos",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/anunciosguardados",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/comprobarToken",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/guardarAnuncios/*",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/borraranuncioguardado/*",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/user",
        "methods": ["DELETE", "PUT"],
        "action": "allow"
      }
    ]
  },
  {
    "group": "NON_ROLE",
    "permissions": [
      {
        "resource": "/anuncios",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/anuncios/*",
        "methods": ["GET"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/user",
        "methods": ["POST"],
        "action": "allow"
      },
      {
        "resource": "/proyecto/login",
        "methods": ["POST"],
        "action": "allow"
      }
    ]
  }
]
